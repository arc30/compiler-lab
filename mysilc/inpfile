type

marks
{
	int phymark;
	int mathmark;
}

person
{
	int age;
	str name;
	marks mark;
	person next;
}



endtype

	decl
		int n,fibo(int n);
		person p1;
		marks p2;
	enddecl

	int fibo(int n){
		decl
			int res;
		enddecl
		begin
			if(n==0)then
				res=0;
			else
				if(n==1)then
					res=1;
				else
					res=fibo(n-1)+fibo(n-2);
				endif;
			endif;
			return res;
		end
	}
	int main(){
		decl
			int result;
			marks m3;
		enddecl
		begin
			read(n);
			p1.name = "ammu";
			p1.age = 21;
			p1.next = null;
		end
	}
